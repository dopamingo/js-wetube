{
  "name": "wetube",
  "version": "1.0.0",
  "description": "cloning youtube with vanillajs and nodejs",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "tunnel": "lt --port 4000",
    "start": "node build/init.js"
  },
  "author": "jung",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/node": "^7.13.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "aws-sdk": "^2.852.0",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "connect-mongodb-session": "^2.4.1",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.1.2",
    "helmet": "^4.3.1",
    "mongoose": "^5.11.11",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "parser": "^0.1.4",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-kakao": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "path": "^0.12.7",
    "pug": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "babel-eslint": "^10.1.0",
    "css-loader": "^1.0.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "mini-css-extract-plugin": "^1.3.4",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.4",
    "sass-loader": "^7.3.1",
    "style-loader": "^2.0.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.4.0"
  },
  "browserslist": [
    "defaults"
  ]
}
